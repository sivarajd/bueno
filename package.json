{
  "name": "bueno",
  "version": "0.2.0",
  "description": "A Bun-Native Full-Stack Framework",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "bueno": "./dist/cli/index.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./cli": {
      "types": "./dist/cli/index.d.ts",
      "import": "./dist/cli/index.js"
    }
  },
  "scripts": {
    "dev": "bun run --watch src/index.ts",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "test:coverage": "bun test --coverage",
    "build": "bun build ./src/index.ts ./src/cli/bin.ts --outdir ./dist --target bun && mv ./dist/cli/bin.js ./dist/cli/index.js",
    "build:cli": "bun build ./src/cli/bin.ts --outdir ./dist/cli --target bun && mv ./dist/cli/bin.js ./dist/cli/index.js",
    "typecheck": "tsc --noEmit",
    "lint": "bunx biome check ./src",
    "cli": "bun run src/cli/bin.ts"
  },
  "keywords": [
    "bun",
    "framework",
    "fullstack",
    "http",
    "router",
    "rpc"
  ],
  "author": "Sivaraj D <sivarajd@gmail.com>",
  "license": "MIT",
  "devDependencies": {
      "@biomejs/biome": "^1.9.0",
      "@types/bun": "latest",
      "typescript": "^5.3.0"
    },
  "peerDependencies": {
    "zod": "^4.0.0",
    "valibot": "^1.0.0",
    "arktype": "^2.0.0",
    "typia": "^7.0.0"
  },
  "peerDependenciesMeta": {
    "zod": { "optional": true },
    "valibot": { "optional": true },
    "arktype": { "optional": true },
    "typia": { "optional": true }
  },
  "engines": {
    "bun": ">=1.3.0"
  }
}
